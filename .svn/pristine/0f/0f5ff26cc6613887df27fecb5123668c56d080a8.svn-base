package cn.weidai.pojo;

import cn.weidai.tools.PageSupport;

public class Base extends PageSupport{
	public Integer id;

	public Integer starNum;//分页的起始行
	public Integer pageSize;//每页显示多少行
	public String sortName; //需要排序的字段
	public String sortType;//排序方式
	public Integer pageIndex;//当前页码

	public Integer getPageIndex() {
		return pageIndex;
	}
	public void setPageIndex(Integer pageIndex) {
		this.pageIndex = pageIndex;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getStarNum() {
		return starNum;
	}
	public void setStarNum(Integer starNum) {
		this.starNum = starNum;
	}
	
	public String getSortName() {
		return sortName;
	}
	public void setSortName(String sortName) {
		this.sortName = sortName;
	}
	public String getSortType() {
		return sortType;
	}
	public void setSortType(String sortType) {
		this.sortType = sortType;
	}


	
}
