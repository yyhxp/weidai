.alertbox {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    display: none;
    z-index: 2000;
}

.alertbox .shadow {
    width: 100%;
    height: 100%;
    background-color: #000;
    opacity: 0.7;
    filter: alpha(opacity=70);
}

.alertbox .shadows {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    display: none;
}

.alertbox .shadows .shadows-bg {
    width: 100%;
    height: 100%;
    background-color: #666;
    opacity: 0.7;
    filter: alpha(opacity=70);
    position: fixed;
    top: 0;
    left: 0;
}

