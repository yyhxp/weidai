package cn.weidai.pojo;
/**
 * 车辆信息
 * @author 6Q
 * 2018-8-8上午9:01:49
 */
public class Car extends Base{
	
	private Integer cId;  //车辆id
	private Integer boId;   //借款用户id
	private String cType; //车辆型号
	private Double cPrice; //购买价格
	private String cPlateNumber; //车牌号
	private Double cMortgagePrice; //抵押价格
	private Double cDrivingkm; //行驶公里
	private  Borrower  borrower;  //借款用户
	
	public Car() {
	}
	
	public Borrower getBorrower() {
		return borrower;
	}

	public void setBorrower(Borrower borrower) {
		this.borrower = borrower;
	}

	public Integer getcId() {
		return cId;
	}
	public void setcId(Integer cId) {
		this.cId = cId;
	}
	public Integer getBoId() {
		return boId;
	}
	public void setBoId(Integer boId) {
		this.boId = boId;
	}
	public String getcType() {
		return cType;
	}
	public void setcType(String cType) {
		this.cType = cType;
	}
	public Double getcPrice() {
		return cPrice;
	}
	public void setcPrice(Double cPrice) {
		this.cPrice = cPrice;
	}
	public String getcPlateNumber() {
		return cPlateNumber;
	}
	public void setcPlateNumber(String cPlateNumber) {
		this.cPlateNumber = cPlateNumber;
	}
	public Double getcMortgagePrice() {
		return cMortgagePrice;
	}
	public void setcMortgagePrice(Double cMortgagePrice) {
		this.cMortgagePrice = cMortgagePrice;
	}
	public Double getcDrivingkm() {
		return cDrivingkm;
	}
	public void setcDrivingkm(Double cDrivingkm) {
		this.cDrivingkm = cDrivingkm;
	}
	@Override
	public String toString() {
		return "Car [cId=" + cId + ", boId=" + boId + ", cType=" + cType
				+ ", cPrice=" + cPrice + ", cPlateNumber=" + cPlateNumber
				+ ", cMortgagePrice=" + cMortgagePrice + ", cDrivingkm="
				+ cDrivingkm + ", borrower=" + borrower + "]";
	}
	
	
}
