/**
 * center normal style.
 */
body, p, div, h1, h2, h3, h4, h5, ul, li, dl, dt, dd, img, span, table, tr, td, th, hr, form, ol, tt, iframe, header, article, footer, tt {
    margin: 0;
    padding: 0; }

h3 {
    font-weight: normal; }

ul, dl {
    list-style: none; }

a {
    color: #555;
    text-decoration: none;
    outline: 0; }

a.blue {
    color: #3db1fa; }

.clearfix {
    zoom: 1;
    display: block; }

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden; }

.visible {
    visibility: visible; }

.unvisible {
    visibility: hidden; }

.tc {
    text-align: center; }

.tl {
    text-align: left; }

.mt10 {
    margin-top: 10px; }

/******fonts*******/
.fz12 {
    font-size: 12px; }

.fz14 {
    font-size: 14px; }

/*****fonts-face*****/
@font-face {
    font-family: DIN-Medium;
    src: url("../fonts/DINPro-Medium.otf"); }
.DinFont {
    font-family: DIN-Medium,sans-serif; }

/******button******/
.center_btn_a, .center_btn_b {
    font-size: 14px;
    background-color: #36adf7;
    border-radius: 5px;
    color: #ffffff;
    border: none;
    cursor: pointer; }

.center_btn_a:hover, .center_btn_b:hover {
    background-color: #25a7f7; }

.center_btn_a:active, .center_btn_b:active {
    background-color: #0c9df7; }

.center_btn_a {
    width: 250px;
    height: 40px;
    line-height: 40px; }

.center_btn_b {
    width: 160px;
    height: 30px;
    line-height: 40px; }

/******line******/
.normal_border {
    border-bottom: 1px solid #eeeeee; }

/*******fontColor********/
.grayLightColor {
    color: #cccccc; }

.grayDarkColor {
    color: #9e9e9e; }

.coffeeColor {
    color: #ca7456; }

.blueColor {
    color: #36adf7; }

.blackColor {
    color: #212121; }

.orangeColor {
    color: #ff5722; }

.redColor {
    color: #f24343; }

.whiteColor {
    color: #ffffff; }

/******fonts-face******/
