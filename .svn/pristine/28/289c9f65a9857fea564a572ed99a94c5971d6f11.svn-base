/*右侧浮动样式*/

.floatBtn {
    position: fixed;
    top:40%;
    right: 20px;
    z-index: 1000;
    width: 50px
}
a.backTop {
    background-position: -160px -240px;
    background-color: #36adf7;
    bottom: 120px;
    cursor: pointer;
    display: none;
    height: 50px;
    line-height: 50px;
    position: fixed;
    right: 5px;
    width: 50px;
    z-index: 10
}

a.backTop:hover {
    background-color: #25a7f7;
}
a.backTop i {
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -13px;
    margin-top: -6px;
    width: 26px;
    height: 12px;
    background-position: -160px -240px
}
.floatBtn ul li {
    width: 40px;
    height: 40px;
    background-color: #36adf7;
    margin-bottom: 1px;
    color: white;
    font-size: 16px;
    position: relative;
    float: left;
    border-radius: 6px;
}
.floatBtn ul li a.BtnName {
    display: block;
    width: 40px;
    height: 40px;
    text-indent: -9999px;
    position: relative;
    cursor: pointer
}
.floatBtn ul li a.what {
    position: absolute;
    top: 0;
    left: 0;
    width: 0;
    background-color: #2997dc;
    font-size: 14px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    z-index: -1000;
    display: none
}
.floatBtn ul li a i {
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -11px;
    margin-top: -11px;
    background-image: url(../img/icon.png);
    background-repeat: no-repeat;
}

.floatBtn ul li.kefu a i {
    width: 23px;
    height: 23px;
    background-position: 0 -240px;
}
.floatBtn ul li.compute a i {
    width: 23px;
    height: 23px;
    background-position: -40px -240px;
}
.floatBtn ul li.feedback a i {
    width: 18px;
    height: 18px;
    background-position: -200px -240px;
    margin-left: -8px;
}
.floatBtn ul li.watch a i {
    width: 23px;
    height: 19px;
    background-position: -80px -240px;
    margin-top: -10px
}

.floatBtn ul li.app a i {
    width: 12px;
    height: 23px;
    background-position: -120px -240px;
    margin-left: -6px
}
.floatBtn ul li.top a i {
    width: 19px;
