#header {
    min-width: 1200px;
    border-bottom: #e6e6e6 solid 1px;
    background: #fff;
    height: 65px;
}
.logo {
    float: left;
    margin: 0 0 0 12px;
    display: inline
}
.headBanner {
    float: left;
    margin-left: 15px;
    display: inline;
    height: 65px;
}
.headBanner img {
    height: 93%;
}
#nav {
    float: right;
}
#nav li {
    border: 1px solid #ffffff;
    border-radius: 3px 3px 3px 3px;
    float: left;
    height: 30px;
    line-height: 30px;
    width: 94px;
    margin: 16px 5px;
    display: inline;
    font-size: 16px;
    position: relative;
    z-index: 101;
    text-align: center;
    -webkit-transition: all .5s ease 0s;
    transition: all .5s ease 0s;
    padding: 0;
}
#nav li.curr a {
    color: #ff5722;
}
#nav li.hover {
    border: 1px solid #c7c7c7;
}
#nav li.hover>a:hover {
    color: #555;
}
#nav li.curr.hover>a:hover {
    color: #ff5722;
}
#nav li.curr:before {
    content: "";
    width: 7px;
    height: 7px;
    background-image: url(../../common/img/navStyle.png);
    display: inline-block;
    margin-right: 5px;
    position: relative;
    top: -2px;
}
#nav li.curr.hover:before {
    display: none;
}
#nav li .show {
    -webkit-transition: all .5s ease 0s;
    transition: all .5s ease 0s;
    border-radius: 0 0 3px 3px;
    -webkit-transform: translateY(-10px);
    transform: translateY(-10px);
    opacity: 0;
    position: absolute;
    line-height: 24px;
    top: 30px;
    left: -1px;
    background: #fff;
    border: #c7c7c7 solid 1px;
    border-top: 0;
    width: 84px;
    padding: 4px 5px 6px 5px;
    visibility: hidden;
    font-size: 14px;
}
#nav li .show p a {
    float: left;
    color: #555;
    font-weight: normal;
    width: 80%;
    height: 24px;
    padding: 0 10%;
    margin-top: 2px;
    display: block;
    position: relative;
    overflow: hidden;
}
#nav li .show p a:hover {
    color: #ff5722;
    text-decoration: none;
    cursor: pointer
}
#nav li .show p a i {
    position: absolute;
    top: -40px;
    left: -10px;
    height: 100px;
    width: 100px;
    border-radius: 50%;
    background: #f3f3f3;
    -webkit-transform: scale(0);
    transform: scale(0);
    -webkit-transition: -webkit-transform .5s ease 0s;
    transition: -webkit-transform .5s ease 0s;
    transition: transform .5s ease 0s;
    transition: transform .5s ease 0s, -webkit-transform .5s ease 0s;
    visibility: hidden;
}
#nav li .show p a:hover i {
    -webkit-transform: scale(1);
    transform: scale(1);
    visibility: visible;
}
#nav li .show p a span {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}
#nav li.hover .show {
    visibility: visible;
    opacity: 1;
    -webkit-transform: translateY(-2px);
    transform: translateY(-2px);
}
