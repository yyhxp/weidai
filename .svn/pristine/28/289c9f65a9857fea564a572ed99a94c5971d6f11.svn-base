        display: none;
    }
    #floatBtn{
        background: rgba(255,255,255,0);
        border-left:0;
    }
    #floatBtn ul li.app{
        background:#fff;
        opacity: 0.9;
        border:1px solid #e8e8e8;
        border-right:0;
        border-radius: 0;
    }
    #floatBtn ul li.app:hover{
        background:#17a4fa;
    }
    #floatBtn ul li.app:hover a i{
        color:#fff;
    }
    #floatBtn ul li.app:hover span{
    color:#fff;
    }
    #floatBtn ul li.stayDis{
        height:80px;
    }
}

